<h2>Login To page </h2>

<%= form_tag '/login/create' do %>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </div>
  <div>
  <%= form.label :password %><br>
  <%= form.text_field :password %>
  </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>

