<h1>Create New Usser</h1>

<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>
  <div>
  <%= form.label :usertype %><br>
  <%= form.text_field :usertype %>
</div>
<div>
<%= form.label :password %><br>
<%= form.text_field :password %>
</div>

  <div>
    <%= form.submit %>
  </div>
<% end %>